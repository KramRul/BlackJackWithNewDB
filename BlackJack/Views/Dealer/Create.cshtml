@{
    ViewData["Title"] = "Register";
}
@model BlackJack.BLL.Models.DealerViewModel

<h2>Регистрация нового дилера</h2>
<form method="post" asp-controller="Dealer" asp-action="Create">
    <div asp-validation-summary="ModelOnly"></div>
    <div>
        <label asp-for="Id"></label><br />
        <input asp-for="Id" />
        <span asp-validation-for="Id"></span>
    </div>
    <div>
        <input type="submit" value="Регистрация" />
    </div>
</form>

